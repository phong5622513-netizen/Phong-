BLOXFUITS -- Blox Fruits Full Auto Farm Script (Krnl Ready)
-- Features:
-- Auto Quest, Auto Farm, Auto Skill, Auto Equip Weapon/DF, Auto Return, Level Filter
-- GUI toggle từng chức năng

-- Services
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

-- GUI Setup
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
local Frame = Instance.new("Frame", ScreenGui)
Frame.Size = UDim2.new(0, 220, 0, 350)
Frame.Position = UDim2.new(0, 10, 0, 50)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

local function CreateButton(name, pos, callback)
    local btn = Instance.new("TextButton", Frame)
    btn.Size = UDim2.new(0, 200, 0, 30)
    btn.Position = pos
    btn.Text = name
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
    btn.MouseButton1Click:Connect(callback)
end

-- Toggles
local AutoFarm = false
local AutoQuest = false
local AutoSkill = false
local AutoEquip = false
local AutoReturn = false

CreateButton("Auto Farm", UDim2.new(0,
